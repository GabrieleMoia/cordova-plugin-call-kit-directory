<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-call-kit-directory" version="1.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>CallKitDirectoryPlugin</name>
    <js-module name="CallKitDirectoryPlugin" src="www/CallKitDirectoryPlugin.js">
        <clobbers target="CallKitDirectoryPlugin" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="CallKitDirectoryPlugin">
                <param name="ios-package" value="CallKitDirectoryPlugin" />
            </feature>
        </config-file>
        <source-file src="src/ios/CallKitDirectoryPlugin.swift" />
    </platform>
</plugin>